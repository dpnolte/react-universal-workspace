{
  "name": "@mono/desktop",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "homepage": "./",
  "scripts": {
    "build": " react-app-rewired build",
    "build:prod": "react-app-rewired build; lerna run desktop-create-package --scope @mono/scripts; electron-packager dist mono --overwrite --asar; rimraf dist",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "start": "yarn concurrently 'yarn server:start' 'yarn wait-on http://localhost:3000/; yarn electron:dev'",
    "start:prod": "yarn concurrently 'yarn server:start' 'yarn wait-on http://localhost:3000/; yarn electron:prod'",
    "start:profile": "yarn concurrently 'yarn server:profile' 'yarn wait-on http://localhost:3000/; yarn electron:profile'",
    "server:start": "cross-env BROWSER=none react-app-rewired start",
    "server:profile": "cross-env BROWSER=none PROFILING=true react-app-rewired start",
    "electron:dev": "cross-env NODE_ENV=development ELECTRON_URL=http://localhost:3000 electron index.js",
    "electron:prod": "cross-env NODE_ENV=production electron index.js",
    "electron:profile": "cross-env NODE_ENV=development PROFILING=true ELECTRON_URL=http://localhost:3000 electron index.js",
    "check-types": "tsc --project ./tsconfig.json"
  },
  "dependencies": {
    "@mono/core": "0.0.1",
    "@mono/ui": "0.0.1",
    "@mono/ui-dom": "0.0.1",
    "@react-navigation/core": "^3.4.1",
    "@react-navigation/web": "git+https://github.com/dpnolte/react-navigation-web",
    "electron-window-state": "^5.0.3",
    "mobx": "^5.9.4",
    "mobx-react-lite": "^1.3.2",
    "nedb": "^1.8.0",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-native-svg-web": "^1.0.1",
    "react-native-vector-icons": "^6.4.2",
    "react-native-web": "^0.11.2",
    "react-native-web-linear-gradient": "^1.0.5",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@jest-runner/electron": "^1.1.1",
    "@mono/scripts": "0.0.1",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-native": "^0.57.46",
    "@types/rimraf": "^2.0.2",
    "asar": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "concurrently": "^4.1.0",
    "core-js": "2.6.5",
    "cross-env": "^5.2.0",
    "electron": "^13.6.6",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^13.1.1",
    "ignore-loader": "^0.1.2",
    "jest": "^24.7.1",
    "react-app-rewired": "^2.1.3",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.4",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^react-native-svg$": "react-native-svg-web"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
